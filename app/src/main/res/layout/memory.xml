<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EFF3FB">

    <Button
        android:id="@+id/button5"
        android:layout_width="125dp"
        android:layout_height="115dp"
        android:layout_marginStart="252dp"
        android:layout_marginLeft="252dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/memor_card"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbarMemory" />

    <Button
        android:id="@+id/button8"
        android:layout_width="125dp"
        android:layout_height="115dp"
        android:layout_marginStart="252dp"
        android:layout_marginLeft="252dp"
        android:layout_marginTop="489dp"
        android:background="@drawable/memor_card"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbarMemory" />

    <Button
        android:id="@+id/button7"
        android:layout_width="125dp"
        android:layout_height="115dp"
        android:layout_marginStart="252dp"
        android:layout_marginLeft="252dp"
        android:layout_marginTop="338dp"
        android:background="@drawable/memor_card"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbarMemory" />

    <Button
        android:id="@+id/button6"
        android:layout_width="125dp"
        android:layout_height="115dp"
        android:layout_marginStart="252dp"
        android:layout_marginLeft="252dp"
        android:layout_marginTop="187dp"
        android:background="@drawable/memor_card"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbarMemory" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbarMemory"
        android:layout_width="0dp"
        android:layout_height="67dp"
        android:background="@drawable/memory_bar"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/cardTitleMemory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="155dp"
        android:layout_marginLeft="130dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="156dp"
        android:layout_marginRight="156dp"
        android:fontFamily="@font/roboto_slab_bold"
        android:text="Memory"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/toolbarMemory" />

    <TextView
        android:id="@+id/memoryTime"
        android:layout_width="39dp"
        android:layout_height="21dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:fontFamily="@font/roboto_slab_bold"
        android:text="03:00"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/toolbarMemory"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/button1"
        android:layout_width="125dp"
        android:layout_height="115dp"
        android:layout_marginStart="36dp"
        android:layout_marginLeft="36dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/memor_card"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbarMemory" />

    <Button
        android:id="@+id/button2"
        android:layout_width="125dp"
        android:layout_height="115dp"
        android:layout_marginStart="36dp"
        android:layout_marginLeft="36dp"
        android:layout_marginTop="187dp"
        android:background="@drawable/memor_card"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbarMemory" />

    <Button
        android:id="@+id/button3"
        android:layout_width="125dp"
        android:layout_height="115dp"
        android:layout_marginStart="36dp"
        android:layout_marginLeft="36dp"
        android:layout_marginTop="338dp"
        android:background="@drawable/memor_card"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbarMemory" />

    <Button
        android:id="@+id/button4"
        android:layout_width="125dp"
        android:layout_height="115dp"
        android:layout_marginStart="36dp"
        android:layout_marginLeft="36dp"
        android:layout_marginTop="489dp"
        android:background="@drawable/memor_card"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbarMemory" />

    <Button
        android:id="@+id/button52"
        android:layout_width="22dp"
        android:layout_height="20.47dp"
        android:background="@drawable/vector"
        app:layout_constraintBottom_toBottomOf="@+id/toolbarMemory"
        app:layout_constraintEnd_toStartOf="@+id/cardTitleMemory"
        app:layout_constraintHorizontal_bias="0.218"
        app:layout_constraintStart_toStartOf="@+id/toolbarMemory"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>